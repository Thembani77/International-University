<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
        android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".startui">

    <View
        android:id="@+id/view2"
        android:layout_width="0dp"
        android:layout_height="200sp"
        android:layout_margin="0sp"
        android:layout_marginTop="16sp"

        android:layout_marginBottom="52dp"
        android:background="@drawable/newbg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_marginTop="15sp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_alignParentEnd="true"
        app:layout_constraintEnd_toEndOf="parent"
        android:fontFamily="serif"
        android:text="Our IT Training Programs"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottieAnimationView"
        android:layout_marginTop="16sp"

        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/newbg2" />

    <Button
        android:id="@+id/higherC"
        android:layout_width="match_parent"
        android:layout_height="80sp"
        android:layout_margin="0sp"
        android:paddingLeft="33sp"
        android:background="@drawable/coursebutton"
        android:fontFamily="serif"
        android:onClick="higherC"
        android:text="Higher Certficate"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="19sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:layout_constraintVertical_bias="0.277" />

    <View
        android:id="@+id/view5"
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:layout_marginStart="32dp"
        android:background="@drawable/diploma"
        app:layout_constraintBottom_toBottomOf="@+id/shortcourse"
        app:layout_constraintStart_toStartOf="@+id/shortcourse"
        app:layout_constraintTop_toTopOf="@+id/shortcourse"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/shortcourse"
        android:layout_width="match_parent"
        android:layout_height="80sp"
        android:layout_margin="0sp"
        android:background="@drawable/coursebutton"
        android:fontFamily="serif"
        android:onClick="shortcourse"
        android:text="shortcourse"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="19sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:layout_constraintVertical_bias="0.044" />

    <View
        android:id="@+id/view3"
        android:layout_height="70sp"
        android:layout_width="70sp"
        android:layout_marginStart="32dp"
        android:background="@drawable/hc"
        app:layout_constraintBottom_toBottomOf="@+id/higherC"
        app:layout_constraintStart_toStartOf="@+id/higherC"
        app:layout_constraintTop_toTopOf="@+id/higherC" />

    <Button
        android:id="@+id/diploma"
        android:layout_width="match_parent"
        android:layout_height="80sp"
        android:layout_margin="0sp"
        android:layout_marginStart="192dp"
        android:background="@drawable/coursebutton"
        android:fontFamily="serif"
        android:onClick="diploma"
        android:text="diploma"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="19sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:layout_constraintVertical_bias="0.514" />

    <View
        android:id="@+id/view6"
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:layout_marginStart="32dp"
        android:background="@drawable/sc"
        app:layout_constraintBottom_toBottomOf="@+id/diploma"
        app:layout_constraintStart_toStartOf="@+id/diploma"
        app:layout_constraintTop_toTopOf="@+id/diploma"
        app:layout_constraintVertical_bias="0.555" />

    <Button
        android:id="@+id/bachelor"
        android:layout_width="match_parent"
        android:layout_height="80sp"
        android:layout_margin="0sp"
        android:background="@drawable/coursebutton"
        android:fontFamily="serif"
        android:onClick="bachelor"
        android:text="bachelor"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="19sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:layout_constraintVertical_bias="0.749" />

    <View
        android:id="@+id/view8"
        android:layout_height="70sp"
        android:layout_width="70sp"
        android:layout_marginStart="32dp"
        android:background="@drawable/bc"
        app:layout_constraintBottom_toBottomOf="@+id/bachelor"
        app:layout_constraintStart_toStartOf="@+id/bachelor"
        app:layout_constraintTop_toTopOf="@+id/bachelor" />


    <Button
        android:id="@+id/button4"
        android:layout_width="match_parent"
        android:layout_height="80sp"
        android:layout_margin="0sp"
        android:layout_marginLeft="0sp"
        android:layout_marginBottom="0sp"
        android:background="@drawable/buttonui"
        android:elevation="20sp"
        android:fontFamily="serif"
        android:onClick="returning"
        android:padding="12sp"
        android:text="Returning Student"
        android:textAlignment="center"
        android:textSize="19sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />

</androidx.constraintlayout.widget.ConstraintLayout>